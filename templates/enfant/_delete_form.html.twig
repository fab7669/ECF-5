<form method="post" action="{{ path('app_enfant_delete', {'id': enfant.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet enfant ?');" style="display: inline-block">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ enfant.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>